<h1>Edit event</h1>
<%= form_for @event do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <%= f.label :venue %>
  <%= f.text_area :venue %>
   <p>
    <%= f.label :date %>
    <%= f.date_select :date %>
  </p>
   <p>
    <%= f.label :time %>
    <%= f.time_select :time %>
  </p>
  <p>
    <%= f.submit 'Update', class: 'button success' %>
  </p>
<% end %>
