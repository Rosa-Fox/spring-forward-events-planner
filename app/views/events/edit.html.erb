 <div class="twelve columns">
   <div class="row">
    <div class="large-4 columns" %>
      <br>
      <h1><img class="logo" src="/img/logo.png">Edit Event</h1>
      </br>
    </div>
  </div>
</div>
<%= form_for @event do |f| %>
<div class="row text-center">
  <div class="large-12 columns" %>
    <div class="row text-center">
      <div class="large-4 columns">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
    </div>
    <div class="row text-center">
      <div class="large-4 columns">
        <%= f.label :venue %>
        <%= f.text_field :venue %>
      </div>
    </div>
    <div class="row text-center">
      <div class="large-2 columns">
        <%= f.label :date %>
        <%= f.date_select :date %>
      </div>
    <div class="large-2 columns">
      <%= f.label :time %>
      <%= f.time_select :time %>
    </div>
    <div class="row ">
      <div class="large-12 columns" %>
        <%= f.submit 'Update', class: 'button success button expand round' %>
      </div>
    </div>
    </div>
  </div>
</div>
<% end %>